
@{
    ViewData["Title"] = "Index";
}

<h2>Log In</h2>

<form asp-action="SignIn" asp-controller="LogIn">
    <p>User Id: <input name="UserId" type="text" class="form-control" required\> </p>
    <p>Password: <input name="Password" type="password" class="form-control" required> </p>
    <button type="submit" class="btn btn-default">Log In </button>
</form>
@ViewBag.info